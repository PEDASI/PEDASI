server {
    listen 80;
    server_name localhost;

    location /favicon.ico {
        alias /var/www/pedasi/static/img/favicon.ico;
    }
    location /static/ {
        alias /var/www/pedasi/static/;
    }

    location / {
        include     uwsgi_params;
        uwsgi_pass  unix:/run/uwsgi/pedasi.sock;
    }
}
